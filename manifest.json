{
   "name" : "QR Sync",
   "version" : "1.0.0",
   "description" : "上传手机信息到浏览器",
   "browser_action" : {
      "default_popup" : "popup.html",
      "default_title" : "QR Sync"
   },
   "manifest_version": 2,
   "permissions": ["contextMenus", "cookies", "tabs"],
   "background": {
        "scripts": ["js/jquery.min.js","js/bg.js"]
   },
   "content_scripts": [{
     "js": ["js/jquery.min.js", "js/popup.js"],
     "matches": ["http://*/*", "https://*/*"]
   }]
}
